   "source": [
    "# Fibonacci even number sum program\n",
    "a = 1\n",
    "b = 2\n",
    "result = 2  # start with 2 since it's the first even Fibonacci number\n",
    "c = 0\n",
    "while a + b < 100:\n",
    "    c = a + b\n",
    "    if c % 2 == 0:\n",
    "        result = result + c\n",
    "    a = b\n",
    "    b = c\n",
    "\n",
    "print(result)\n",
    "\n"
   ]
